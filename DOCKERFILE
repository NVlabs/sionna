FROM tensorflow/tensorflow:2.9.1-gpu-jupyter
EXPOSE 8888
COPY . /tmp/
WORKDIR /tmp/
RUN make install
RUN rm -R /tf/* && cp /tmp/examples/* /tf
RUN rm -R /tmp
RUN chmod 777 -R /tf
WORKDIR /tf
CMD ["jupyter", "notebook", "--port=8888", "--no-browser", "--NotebookApp.token=''", "--ip=0.0.0.0", "--allow-root", "--NotebookApp.allow_origin='*'"]
